{
  "name": "tumblin-dice-counter",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "typecheck": "tsc --noEmit",
    "deploy": "yarn build && tar -cvf ./deploy.tar ./captain-definition ./dist/* && npx caprover deploy -t ./deploy.tar --default"
  },
  "dependencies": {
    "caprover": "^2.2.3",
    "clsx": "^1.1.1",
    "immer": "^9.0.12",
    "interpolated-material-colors": "^2.0.1",
    "nanoid": "^3.3.1",
    "phosphor-react": "^1.4.0",
    "react": "^17.0.0",
    "react-cool-onclickoutside": "^1.7.0",
    "react-dom": "^17.0.0",
    "react-spring": "^9.4.3",
    "zustand": "^3.7.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.9.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "postcss": "^8.4.6",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.5",
    "vite": "^2.8.4"
  }
